<div class="astrology-chart-container">
  <div class="margin-container">
    <img src="https://res.cloudinary.com/dgeodyyf7/image/upload/v1584321394/Astromatch_Elementos-02_yet3zo.png">

    <%= simple_form_for @astrology_chart do |f| %>

      <%= f.input :birthdate, input_html: { class: "datepicker" }, as: :string %>

      <%= f.input :birth_city %>

      <%= f.input :state_id, collection: @states %>

      <%= f.button :submit, "Gerar Mapa", class:"btn profile-btn col-sm-4 mt-1" %>
    <% end %>
  </div>
</div>
